{% extends 'connected-base.html.twig' %}
{%  block stylesheets %}

    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('@Ake') }}">
{% endblock %}

{% block content %}
<div class="products expanded">
    <section class="product-grid">
        <header class="product-grid__header">
        </header>
        <div class="container">
            <div class="admin-form-container">
                <div class="admin-form">
                    <div class="col-12">
                        {% for message in app.flashes('success') %}
                            <div class="alert alert-success" role="alert">
                                {{ message }}
                            </div>
                        {% endfor %}
                        {% for message in app.flashes('error') %}
                            <div class="alert alert-danger" role="alert">
                                {{ message | nl2br }}
                            </div>
                        {% endfor %}
    <h1>{{ title }}</h1>
                        <hr />
    {{ form_start(form) }}
    {{ form_widget(form) }}
                        <br />
    <button type="submit" class="btn btn-success btn-sm">Save</button>
                        <a href="{{ path('akeneo_presales_custom_app_essentials_ui_extension_index') }}" class="btn btn-info btn-sm">Back to list</a>
                        {{ form_end(form) }}
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
{% endblock %}
