{
    "name": "akeneo-presales/custom-app-essentials-bundle",
    "description": "Akeneo Custom Apps Multi tenant Service classes, Event platform management UI and more",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AkeneoPresales\\CustomAppEssentialsBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Florian Lasdoulours",
            "email": "florian.lasdoulours@akeneo.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-kernel": "^6.4",
        "doctrine/orm": "^2.14",
        "lcobucci/jwt": "^4.3.0",
        "guzzlehttp/guzzle": "^7.9",
        "symfony/framework-bundle": "^6.4",
        "symfony/form": "^6.4",
        "akeneo/api-php-client": "v11.3.0",
        "idci/graphql-client-bundle": "^1.2",
        "google/cloud-pubsub": "^2.8",
        "symfony/validator": "^6.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
